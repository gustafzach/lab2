typedef struct _Node {
    int number;
    struct _Node* next;
}Node;


Node* getInput(char* message, Node* sumNode, char* array);
Node* setNodes(char* input);
void PrintNode(Node* lista);
void freespace(Node* sumNode);
Node* adder(Node* number1, Node* number2);
